<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.sayani.student_entry.Main3Activity"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://schemas.android.com/apk/res/android ">


    <TextView android:id="@+id/textView" android:layout_width="wrap_content"
        android:layout_height="wrap_content" android:layout_alignParentStart="true"
        android:layout_alignParentTop="true" android:layout_marginStart="13dp"
        android:layout_marginTop="16dp" android:text="Student ID" />

    <TextView android:id="@+id/textView2" android:layout_width="wrap_content"
        android:layout_height="wrap_content" android:layout_alignTop="@+id/textView"
        android:layout_marginStart="26dp" android:layout_toEndOf="@+id/textView"
        android:text="TextView" />

    <Button android:id="@+id/button2" android:layout_width="wrap_content"
        android:layout_height="wrap_content" android:layout_alignBaseline="@+id/button3"
        android:layout_alignBottom="@+id/button3" android:layout_alignStart="@+id/textView14"
        android:onClick="submitClick" android:text="Submit" />

    <Button android:id="@+id/button3" android:layout_width="wrap_content"
        android:layout_height="wrap_content" android:layout_alignStart="@+id/textView13"
        android:layout_below="@+id/textView13" android:layout_marginTop="35dp"
        android:onClick="editClick" android:text="Edit" />

    <TextView android:id="@+id/textView3" android:layout_width="wrap_content"
        android:layout_height="wrap_content" android:layout_alignStart="@+id/textView"
        android:layout_below="@+id/textView" android:layout_marginTop="18dp" android:text="Name" />

    <TextView android:id="@+id/textView4" android:layout_width="wrap_content"
        android:layout_height="wrap_content" android:layout_alignStart="@+id/textView2"
        android:layout_alignTop="@+id/textView3" android:text="TextView" />

    <TextView android:id="@+id/textView5" android:layout_width="wrap_content"
        android:layout_height="wrap_content" android:layout_alignStart="@+id/textView3"
        android:layout_below="@+id/textView3" android:layout_marginTop="33dp" android:text="Sem" />

    <TextView android:id="@+id/textView6" android:layout_width="wrap_content"
        android:layout_height="wrap_content" android:layout_alignStart="@+id/textView4"
        android:layout_alignTop="@+id/textView5" android:text="TextView" />

    <TextView android:id="@+id/textView7" android:layout_width="wrap_content"
        android:layout_height="wrap_content" android:layout_alignStart="@+id/textView5"
        android:layout_below="@+id/editText9" android:text="Branch" />

    <TextView android:id="@+id/textView8" android:layout_width="wrap_content"
        android:layout_height="wrap_content" android:layout_alignStart="@+id/textView6"
        android:layout_alignTop="@+id/textView7" android:text="TextView" />

    <TextView android:id="@+id/textView9" android:layout_width="wrap_content"
        android:layout_height="wrap_content" android:layout_alignStart="@+id/textView7"
        android:layout_below="@+id/textView7" android:layout_marginTop="31dp" android:text="FA" />

    <TextView android:id="@+id/textView10" android:layout_width="wrap_content"
        android:layout_height="wrap_content" android:layout_alignEnd="@+id/textView8"
        android:layout_alignTop="@+id/textView9" android:text="TextView" />

    <TextView android:id="@+id/textView11" android:layout_width="wrap_content"
        android:layout_height="wrap_content" android:layout_alignStart="@+id/textView9"
        android:layout_centerVertical="true" android:text="Phone" />

    <TextView android:id="@+id/textView12" android:layout_width="wrap_content"
        android:layout_height="wrap_content" android:layout_alignBaseline="@+id/textView11"
        android:layout_alignBottom="@+id/textView11" android:layout_alignStart="@+id/textView10"
        android:text="TextView" />

    <TextView android:id="@+id/textView13" android:layout_width="wrap_content"
        android:layout_height="wrap_content" android:layout_alignStart="@+id/textView11"
        android:layout_below="@+id/editText12" android:layout_marginTop="15dp"
        android:text="Email" />

    <TextView android:id="@+id/textView14" android:layout_width="wrap_content"
        android:layout_height="wrap_content" android:layout_alignBottom="@+id/textView13"
        android:layout_alignEnd="@+id/textView12" android:text="TextView" />

    <EditText android:id="@+id/editText8" android:layout_width="wrap_content"
        android:layout_height="wrap_content" android:layout_alignParentEnd="true"
        android:layout_alignTop="@+id/textView4" android:ems="10" android:hint="Enter name"
        android:inputType="textPersonName" />

    <EditText android:id="@+id/editText9" android:layout_width="wrap_content"
        android:layout_height="wrap_content" android:layout_alignStart="@+id/editText8"
        android:layout_below="@+id/editText8" android:ems="10" android:hint="Enter sem"
        android:inputType="textPersonName" />

    <EditText android:id="@+id/editText10" android:layout_width="wrap_content"
        android:layout_height="wrap_content" android:layout_above="@+id/textView10"
        android:layout_alignStart="@+id/editText9" android:ems="10" android:hint="Enter branch"
        android:inputType="textPersonName" />

    <EditText android:id="@+id/editText11" android:layout_width="wrap_content"
        android:layout_height="wrap_content" android:layout_alignStart="@+id/editText10"
        android:layout_below="@+id/editText10" android:ems="10" android:hint="Enter FA"
        android:inputType="textPersonName" />

    <EditText android:id="@+id/editText12" android:layout_width="wrap_content"
        android:layout_height="wrap_content" android:layout_alignStart="@+id/editText11"
        android:layout_below="@+id/editText11" android:ems="10" android:hint="Enter phone"
        android:inputType="textPersonName" />

    <EditText android:id="@+id/editText13" android:layout_width="wrap_content"
        android:layout_height="wrap_content" android:layout_alignStart="@+id/editText12"
        android:layout_below="@+id/editText12" android:ems="10" android:hint="Enter email"
        android:inputType="textPersonName" />
</RelativeLayout>